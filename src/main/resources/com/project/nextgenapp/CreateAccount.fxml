        <Label layoutX="186.0" layoutY="44.0" text="Create Account" textFill="#8ced24">
            <font>
                <Font size="32.0" />
            </font>
        </Label>
        <Label fx:id="test" layoutX="58.0" layoutY="36.0" prefHeight="26.0" prefWidth="80.0" text="Label" textFill="WHITE" />
