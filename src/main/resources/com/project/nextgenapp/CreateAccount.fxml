<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: black;" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.project.nextgenapp.CreateAccountApplication">
    <children>
        <Label layoutX="134.0" layoutY="112.0" text="First Name" textFill="#8ced24">
            <font>
                <Font size="18.0" />
            </font>
        </Label>
        <TextField id="firstName" fx:id="firstName" layoutX="231.0" layoutY="113.0" />
        <Label layoutX="137.0" layoutY="151.0" text="Username" textFill="#8ced24">
            <font>
                <Font size="18.0" />
            </font>
        </Label>
        <TextField id="username" fx:id="username" layoutX="231.0" layoutY="152.0" />
        <Label layoutX="139.0" layoutY="192.0" text="Password" textFill="#8ced24">
            <font>
                <Font size="18.0" />
            </font>
        </Label>
        <PasswordField id="password" fx:id="password" layoutX="231.0" layoutY="193.0" />
        <Button id="signUp" fx:id="signUpButton" layoutX="239.0" layoutY="298.0" mnemonicParsing="false" onAction="#signupButton" style="-fx-background-color: #8ced24;" text="Sign Up">
